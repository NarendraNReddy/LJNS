    agent any
    stages {
        stage('Build'){
            steps{
                sh 'echo Build stage'
            }
        }
    }

    stages{
        stage('Test'){
         steps{
            sh 'echo Test stage'
            }
        }
    }
    stages{
        stage('Deploy'){
         steps{
            sh 'echo Deploy stage'
            }
        }
    }  

    
}